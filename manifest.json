{
  "manifest_version": 3,
  "name": "LinkedIn Profile Collector",
  "version": "1.0",
  "description": "LinkedIn arama sonuçlarından profil verilerini CSV olarak kaydetmenizi sağlar",
  "permissions": [
    "activeTab",
    "downloads",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*.linkedin.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 